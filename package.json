{
  "name": "sentrum",
  "productName": "sentrum",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "./.webpack/main",
  "repository": "git@github.com:patbonecrusher/sentrum.git",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "make:all": "npm run make:mac & npm run mac:linux & npm run mac:win",
    "make:mac": "electron-forge make --platform=darwin",
    "make:linux": "electron-forge make --platform=linux --arch=ia32,x64",
    "make:win": "electron-forge make --platform=win32 --arch=ia32,x64",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts .",
    "outdated": "yarn outdated",
		"upgrade-i": "yarn upgrade-interactive --latest"
  },
  "keywords": [],
  "author": {
    "name": "Pat Laplante",
    "email": "laplante.patrick@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "sentrum"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./public/index.html",
                  "js": "./src/renderer/renderer.tsx",
                  "name": "main_window",
                  "preload": {
                    "js": "./src/main/preload.js"
                  }
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.50",
    "@electron-forge/maker-deb": "6.0.0-beta.50",
    "@electron-forge/maker-rpm": "6.0.0-beta.50",
    "@electron-forge/maker-squirrel": "6.0.0-beta.50",
    "@electron-forge/maker-zip": "6.0.0-beta.50",
    "@electron-forge/plugin-webpack": "6.0.0-beta.50",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "electron": "8.1.1",
    "eslint": "^6.8.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-import": "^2.20.0",
    "fork-ts-checker-webpack-plugin": "^4.1.0",
    "node-loader": "^0.6.0",
    "node-sass": "^4.13.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.13.0",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "electron-debug": "^3.0.1",
    "electron-is-dev": "^1.1.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-util": "^0.14.0",
    "normalize.css": "^8.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.20",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.0"
  }
}
